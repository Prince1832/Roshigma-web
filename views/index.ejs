    <% if (caseStudies.length> 0) { %>
      <section class="container success-stories p-t-75 p-b-75">
        <h2 class="h2 m-b-20">Success stories</h2>
        <% caseStudies.forEach((c, index)=> { %>
          <a href="/case-studies/<%= c.slug %>">
            <div class="<%= index % 2 === 0 ? 'success-stories__card-odd' : 'success-stories__card-even' %>">
              <% if (index % 2 !==0) { %>
                <div class="success-stories__img">
                  <img width="750" height="400" loading="lazy" src="<%= c.image %>" alt="<%= c.title %> - Rhosigma" />
                </div>
                <% } %>

                  <div class="success-stories__textual-content">
                    <h3 class="h3 fc-w fw-6">
                      <%= c.title %>
                    </h3>
                    <p class="p-2 fc-w fw-4">
                      <%= c.description %>
                    </p>
                  </div>
                  <% if (index % 2===0) { %>
                    <div class="success-stories__img">
                      <img width="750" height="400" loading="lazy" src="<%= c.image %>"
                        alt="<%= c.title %> - Rhosigma" />
                    </div>
                    <% } %>
            </div>
          </a>
          <% }); %>
      </section>
      <% }; %>
