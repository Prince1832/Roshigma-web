            <!-- ! Image Popup -->
            <script>
              // Function to show the popup after 5 seconds
              function showPopup() {
                document.getElementById("popupContainer").style.display = "block";
              }

              // Call the showPopup function after 5 seconds
              setTimeout(showPopup, 5000);

              // Function to close the popup
              function closePopup() {
                document.getElementById("popupContainer").style.display = "none";
              }
            </script>
            <!-- ! Image Popup -->

            <!-- !active card -->
            <script>
              document.addEventListener("DOMContentLoaded", function () {
                const cards = document.querySelectorAll(".technologies__grid-slide");

                cards.forEach((card) => {
                  card.addEventListener("click", function () {
                    // Remove 'active' class from all cards
                    cards.forEach((c) => c.classList.remove("active"));

                    // Add 'active' class to the clicked card
                    this.classList.add("active");
                  });
                });
              });
            </script>
            <!-- !active card -->

            <!-- ! Filtering cards -->
            <script>
              document.addEventListener("DOMContentLoaded", function () {
                const filterButtons = document.querySelectorAll(
                  ".silder-card .swiper-slide"
                );
                const itemsToFilter = document.querySelectorAll(
                  ".slier-234 .swiper-slide"
                );
                const overFlow = document.querySelector(".slier-234");

                function handleFilterButtonClick(button) {
                  const filterClass = button.getAttribute("data-filter");

                  filterButtons.forEach((btn) => btn.classList.remove("active"));
                  itemsToFilter.forEach((item) => item.classList.remove("active"));

                  button.classList.add("active");

                  itemsToFilter.forEach((item) => {
                    item.remove();
                  });

                  itemsToFilter.forEach((item) => {
                    if (item.classList.contains(filterClass) || filterClass === "all") {
                      overFlow.appendChild(item);
                      item.classList.add("active");
                    }
                  });

                  swiper2.update();
                }

                filterButtons.forEach((button) => {
                  button.addEventListener("click", function () {
                    handleFilterButtonClick(button);
                  });
                });

                handleFilterButtonClick(filterButtons[0]);
              });
            </script>
            <!-- ! Filtering cards -->

            <!-- !Changing slier to grid -->
            <script>
              const list = document.querySelector(".technologies__grid-view").classList;
              const listTwo = document.querySelector(".swiper-wrapper").classList;
              var x = window.matchMedia("(max-width: 991px)");

              function myFunction(x) {
                if (x.matches) {
                  list.add("swiper-wrapper");
                  list.remove("technologies__grid-view");
                  listTwo.remove("swiper-wrapper");
                  listTwo.add("technologies__slid-view");
                } else {
                  list.add("technologies__grid-view");
                  list.remove("swiper-wrapper");
                  listTwo.add("swiper-wrapper");
                  listTwo.remove("technologies__slid-view");
                }
              }

              myFunction(x);

              x.addEventListener("change", function () {
                location.reload();
                myFunction(x);
              });
            </script>
