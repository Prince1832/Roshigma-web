            <!-- ! Hero Section -->
            <script>
              const left = document.getElementById("hero-slider__left-side");
              const sliderBtn = document.querySelector(".hero-slider__slider-btn");
              let isDragging = false;

              sliderBtn.addEventListener("mousedown", () => {
                isDragging = true;
              });

              document.addEventListener("mouseup", () => {
                isDragging = false;
              });

              document.addEventListener("mousemove", (e) => {
                if (isDragging) {
                  const newPosition = (e.clientX / window.innerWidth) * 100;
                  left.style.width = `${newPosition}%`;
                }
              });

              sliderBtn.addEventListener("touchstart", () => {
                isDragging = true;
              });

              document.addEventListener("touchend", () => {
                isDragging = false;
              });

              document.addEventListener("touchmove", (e) => {
                if (isDragging) {
                  const newPosition = (e.touches[0].clientX / window.innerWidth) * 100;
                  left.style.width = `${newPosition}%`;
                }
              });
            </script>

