            <p id="phoneError" class="error-message"></p>

          </div>

          <label class="p-weight form-lebel-text label-color" for="email">Email</label>
          <div class="">
            <!-- <input class="p-weight fc-s input-class" type="email" id="email" class="email" placeholder="Enter email" /> -->
