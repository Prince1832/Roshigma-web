          <label class="p-weight form-lebel-text label-color" for="message">Message</label>
          <div class="">
            <textarea class="input-class p-weight fc-s" name="message" id="message" placeholder="Type your requirements"
              autocomplete="off" rows="3"></textarea>
            <p id="messageError" class="error-message"></p>
          </div>

          <button class="submit-btn main-form-btn show-dialog" type="submit">Get a Custome Solution</button>
          <p class="form-message m-t-20"></p>
        </form>
